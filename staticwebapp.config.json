{
  "routes": [
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/static/*",
      "/images/*.{png,jpg,gif,svg}",
      "/css/*",
      "/service-worker.js",
      "/translations.json",
      "manifest.json",
      "/downloads/Setup.pdf"
    ]
  }
}